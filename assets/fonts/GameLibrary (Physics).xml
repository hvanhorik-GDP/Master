<GameLibrary>
	<GameName>Project 1 Phsysics Test</GameName>
	<AssetGroup type="shaders" open="none">
		<AssetRoot>../assets/shaders/</AssetRoot>

		<Asset name="fragmentShader01" type="fragment">
			<AssetFile>fragmentShader01.glsl</AssetFile>
		</Asset>
		<Asset name="vertexShader01" type="vertex">
			<AssetFile>vertexShader01.glsl</AssetFile>
		</Asset>
	</AssetGroup>
	<AssetGroup type="programs" open="none">
		<Asset name="main">
			<AssetPart>fragmentShader01</AssetPart>
			<AssetPart>vertexShader01</AssetPart>
			<Property name="isValid" type="bool" value="true"/>
			<Property name="vNormal" type="Attribute" value="1"/>
			<Property name="vPosition" type="Attribute" value="0"/>
			<Property name="matModel" type="Uniform" value="2"/>
			<Property name="matModelInverseTranspose" type="Uniform" value="3"/>
			<Property name="matProj" type="Uniform" value="0"/>
			<Property name="matView" type="Uniform" value="1"/>
			<Property name="bDoNotLight" type="Uniform" value="12"/>
			<Property name="debugColour" type="Uniform" value="13"/>
			<Property name="diffuseColour" type="Uniform" value="14"/>
			<Property name="eyeLocation" type="Uniform" value="11"/>
			<Property name="specularColour" type="Uniform" value="15"/>
			<Property name="theLights[0].position" type="Uniform" value="4"/>
			<Property name="theLights[0].diffuse" type="Uniform" value="5"/>
			<Property name="theLights[0].specular" type="Uniform" value="6"/>
			<Property name="theLights[0].atten" type="Uniform" value="7"/>
			<Property name="theLights[0].direction" type="Uniform" value="8"/>
			<Property name="theLights[0].param1" type="Uniform" value="9"/>
			<Property name="theLights[0].param2" type="Uniform" value="10"/>
		</Asset>
	</AssetGroup>
	<AssetGroup type="models" open="none">
		<AssetRoot>../assets/models/</AssetRoot>
    <Asset name="pyramid">
      <AssetFile>./Physics/pyramid.ply</AssetFile>
    </Asset>
    <Asset name="pyramid_3">
      <AssetFile>./Physics/pyramid_3.ply</AssetFile>
    </Asset>
    <Asset name="Cube_1_Unit_from_origin_XYZ_n">
      <AssetFile>./Physics/Cube_1_Unit_from_origin_XYZ_n.ply</AssetFile>
    </Asset>
    <Asset name="cube_Hi_Res_xyz_n">
      <AssetFile>./Physics/cube_Hi_Res_xyz_n.ply</AssetFile>
    </Asset>
    <Asset name="cube_Low_Res_xyz_n">
      <AssetFile>./Physics/cube_Low_Res_xyz_n.ply</AssetFile>
    </Asset>
    <Asset name="Sphere_Radius_1_XYZ_n">
      <AssetFile>./Physics/Sphere_Radius_1_XYZ_n.ply</AssetFile>
    </Asset>
    <Asset name="12953_ChocolateRabbit_v1">
      <AssetFile>./Physics/12953_ChocolateRabbit_v1.ply</AssetFile>
    </Asset>
    
  </AssetGroup>

  <AssetGroup type="audios" open="all" subdir="true" subtype="music">
		<AssetRoot>../assets/audios/</AssetRoot>
		</AssetGroup>
  
	<AssetGroup type="videos" open="none">
		<AssetRoot>../assets/videos/</AssetRoot>
	</AssetGroup>
  
	<AssetGroup type="images" open="none">
		<AssetRoot>../assets/images/</AssetRoot>
	</AssetGroup>
  
	<AssetGroup type="fonts" open="none">
		<AssetRoot>../assets/fonts/</AssetRoot>
	</AssetGroup>
	<Objects>
		<Object type="light" name="spotlight" asset_id="">
			<Property name="type" type="string" value="spot"/>
			<Property name="position" type="vec4" value="1,2,3,4"/>
			<Property name="specular" type="vec4" value="1,2,3,4"/>
			<Property name="atten" type="vec4" value="1,2,3,4"/>
			<Property name="direction" type="vec4" value="1,2,3,4"/>
			<Property name="lightType" type="enum" value="0"/>
			<Property name="innerangle" type="float" value=""/>
			<Property name="outer angle" type="float" value=""/>
			<Property name="on" type="bool" value="false"/>
		</Object>
	</Objects>
</GameLibrary>

